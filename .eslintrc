{
  "extends": ["@folio/eslint-config-stripes"],
  "parser": "@babel/eslint-parser",
  "overrides": [
    {
      "files": [
        "**/*.test.js",
        "**/*.test.jsx",
        "**/test/**/*.js"
      ],
      "env": {
        "jest": true
      }
    }
  ],
  "rules": {
    "react-hooks/exhaustive-deps": "off",
    "import/prefer-default-export": 0
  }
}
